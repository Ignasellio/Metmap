<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>

<BorderPane maxHeight="-Infinity"
            maxWidth="-Infinity"
            minHeight="400.0"
            minWidth="500.0"
            prefHeight="800.0"
            prefWidth="1000.0"
            xmlns="http://javafx.com/javafx/19"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="lt.ignassenkus.metmap.controller.ComparingController">
    <top>
        <VBox>
            <padding>
                <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
            </padding>
            <HBox>
                <Button text="Go Back" onAction="#onBackButtonClick"/>
                <Button text="Settings" onAction="#onSettingsButtonClick"/>
                <Region HBox.hgrow="ALWAYS" />
                <Label text="Comparing tool"
                       style="-fx-font-size: 18px; -fx-font-weight: bold; -fx-text-fill: #333333;"/>
            </HBox>
        </VBox>

    </top>

</BorderPane>
