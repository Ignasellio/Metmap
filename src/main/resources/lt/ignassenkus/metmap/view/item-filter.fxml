<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="lt.ignassenkus.metmap.controller.ItemFilterController"
      prefHeight="40" prefWidth="400"
      alignment="CENTER_LEFT"
      spacing="5.0">
    <padding>
        <Insets bottom="5.0" left="10.0" top="5.0" right="5.0"/>
    </padding>

    <Label fx:id="NameLabel"/>
    <Region HBox.hgrow="ALWAYS" />
    <Button text="Edit"/>
    <Button text="Delete"/>
</HBox>
